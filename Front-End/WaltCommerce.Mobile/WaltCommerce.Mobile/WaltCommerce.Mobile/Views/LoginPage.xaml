<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="WaltCommerce.Mobile.Views.LoginPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:ctr="clr-namespace:WaltCommerce.Mobile.Controls"
    xmlns:ffimageloading="clr-namespace:FFImageLoading.Forms;assembly=FFImageLoading.Forms"
    xmlns:i18n="clr-namespace:WaltCommerce.Mobile.Helpers.I18n"
    xmlns:prism="http://prismlibrary.com"
    Title="{Binding Title}"
    prism:ViewModelLocator.AutowireViewModel="True"
    BackgroundColor="#FEFEFE"
    NavigationPage.HasNavigationBar="False">


    <StackLayout
        Margin="20"
        Spacing="30"
        VerticalOptions="CenterAndExpand">


        <ffimageloading:CachedImage
            Background="Transparent"
            CacheDuration="50"
            DownsampleToViewSize="true"
            ErrorPlaceholder="ErrorImage"
            HeightRequest="200"
            LoadingPlaceholder="LoaderImage"
            RetryCount="3"
            RetryDelay="600"
            Source="data:image/jpeg;base64," />
        <ctr:StandardEntry
            BorderColor="#D8DAE9"
            BorderThickness="1"
            CornerRadius="10"
            Keyboard="Email"
            Placeholder="{i18n:Translate PhEmail}"
            Text="{Binding Email}" />
        <ctr:StandardEntry
            BorderColor="#D8DAE9"
            BorderThickness="1"
            CornerRadius="10"
            IsPassword="True"
            Placeholder="{i18n:Translate PhPassword}"
            Text="{Binding Password}" />

        <ActivityIndicator IsRunning="{Binding IsRunning}" />

        <Button
            Padding="10"
            Command="{Binding LoginCommand}"
            IsEnabled="{Binding IsEnabled}"
            Text="{i18n:Translate LogIn}" />


        <Label
            Text="{i18n:Translate SignIn}"
            TextColor="Black"
            TextDecorations="Underline">
            <Label.GestureRecognizers>
                <TapGestureRecognizer Command="{Binding UserRegisterCommand}" />
            </Label.GestureRecognizers>
        </Label>

    </StackLayout>

</ContentPage>